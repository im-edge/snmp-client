{
    "name": "imedge/snmp-client",
    "description": "SNMP Client library",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Gelf",
            "email": "thomas@gelf.net"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "IMEdge\\SnmpClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IMEdge\\Tests\\SnmpClient\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1",
        "imedge/amphp-socket": "^2.3",
        "imedge/snmp-packet": "^0.9.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.3",
        "psr/log": "^3.0",
        "squizlabs/php_codesniffer": "^3.7",
        "symplify/easy-coding-standard": "^12.0.8"
    }
}
